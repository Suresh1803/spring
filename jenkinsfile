pipeline {
  agent any
  tools {
    maven 'maven'
  }
  stages {
    stage ('Build') {
      steps {
        bat 'mvn clean -Dmaven.test.skip package'
      }
    }
    stage ('Run JAR File') {
      steps {
        script {
       sh 'java -jar C:/ProgramData/Jenkins/.jenkins/workspace/demo3/target/sprinboot-restapi-0.0.1-SNAPSHOT.jar'
        }
      }
    }
  }
  post { 
        always { 
            cleanWs()
        }
    }
}
